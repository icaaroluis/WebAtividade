@model WebAtividadeEntrevista.Models.ClienteModel
@{
    ViewBag.Title = "Alterar Cliente";
}
@section scripts{
<script>
        var urlPost = '@Url.Action("Alterar", "Cliente", new { area = "" })';
        var urlListBeneficiario = '@Url.Action("BeneficiarioList", "Cliente", new { id = Model.Id })';
        var urlExcluir = '@Url.Action("ExcluirBeneficiario", "Cliente", new { area = "" })';
        var urlAlteracaoBeneficiario = '@Url.Action("AlterarBeneficiario", "Cliente", new { area = "" })';
        var urlAtualizarBeneficiario = '@Url.Action("AtBeneficiario", "Cliente", new { area = "" })';
        var obj = @Html.Raw(Json.Encode(Model));
        var urlRetorno = '@Url.Action("Index", "Cliente", new { area = "" })';
</script>
    @Scripts.Render("~/bundles/altClientes")
}
<h2>@Html.Raw(ViewBag.Title)</h2>
@Html.Partial("Forms")
